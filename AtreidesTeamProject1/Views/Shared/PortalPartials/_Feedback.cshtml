@{
}
<div class="cardHeader">
    <h2>Recent Feedback</h2>
</div>
<table>
    <thead>
        <tr>
            <td>Name</td>
            <td>Comment</td>

        </tr>
    </thead>
    <tbody>

        @if (Model.ServiceMessages == null)
        {
            <tr><td colspan="2" class="text-center">There are Currently No Products</td></tr>
        }
        else
        {
            @foreach (var fb in Model.FeedBack)
            {
                <tr>
                    <td>@fb.CustomerName</td>
                    <td>@fb.Comment</td>

                </tr>
            }
        } 
        
    </tbody>
</table>

